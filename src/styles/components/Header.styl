.isPage404 
  height 0
  visibility hidden

.Header
  padding: 1% 5% 0% 5%
  font-weight: bold
  display: flex;
  align-items: center;
  flex-direction: column;
  justify-content: center;
  h6
    font-size: 12px
    margin-left: 10px
    margin-top: 3px

  &-img
       display: flex       
       padding: 0
       margin 0
       align-items: center;
  
  &-img img
        height 70px
        width 70px
        border-radius:50% 
        margin-right 7px  

  &-nameLogo p
        padding: 0
        margin 0
        font-size: 20px  
        font-weight: bolder
        
    
  &-message
      padding: 0
      color #2D4249 
       
  a
    color #372F2D
    text-decoration none
    padding: 0 

 

   

 
  


